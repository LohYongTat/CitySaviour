//string splitter
var text = global.prompt; //string to split
var splitBy = ";"; //string to split the first string by
var slot = 0;
var splits; //array to hold all splits
var str2 = ""; //var to hold the current split we're working on building

var i;
for (i = 1; i &lt; (string_length(text)+1); i++) {
    var currStr = string_copy(text, i, 1);
    if (currStr == splitBy) {
        splits[slot] = str2; //add this split to the array of all splits
        slot++;
        str2 = "";
    } else {
        str2 = str2 + currStr;
        splits[slot] = str2;
    }
}

//displays text autowrapped around the coorde(x,y,text,vert space, horizontal space)
global.optimalquestion = str2
draw_text_ext(x,y,splits[0],15,1000)
</string>
